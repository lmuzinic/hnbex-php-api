{
    "name": "lmuzinic/hnbex-php-api",
    "description": "PHP implementation of HNB Exchange Rate Lookup API http://hnbex.eu/api/v1/",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Luka Muzinic",
            "email": "luka@muzinic.net",
            "homepage": "https://luka.muzinic.net"
        }
    ],
    "platform": {
        "php": "7.1"
    },
    "require": {
        "php-http/client-implementation": "^1.0",
        "psr/http-message": "^1.0",
        "php-http/httplug": "^1.1",
        "php-http/message-factory": "^1.0",
        "php-http/discovery": "^1.1",
        "php-http/message": "^1.4",
        "symfony/serializer": "^3.2",
        "symfony/property-access": "^3.2",
        "symfony/options-resolver": "^3.2",
        "webmozart/assert": "^1.2",
        "psr/simple-cache": "^1.0"
    },
    "require-dev": {
        "php-http/mock-client": "^0.3",
        "guzzlehttp/psr7": "^1.3",
        "phpunit/phpunit": "^5.7",
        "php-http/guzzle6-adapter": "v1.1.1",
        "symfony/var-dumper": "^3.2",
        "cache/filesystem-adapter": "^1.0",
        "cache/void-adapter": "^1.0"
    },
    "autoload": {
        "psr-4": { "Hnbex\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Hnbex\\Tests\\": "tests/" },
        "files": [
            "vendor/symfony/var-dumper/Resources/functions/dump.php"
        ]
    }
}
